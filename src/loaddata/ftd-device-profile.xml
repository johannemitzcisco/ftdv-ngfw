<config xmlns="http://tail-f.com/ns/config/1.0">
  <devices xmlns="http://tail-f.com/ns/ncs">
  <profiles>
  <profile>
    <name>ftd</name>
    <connect-timeout>600</connect-timeout>
    <trace>raw</trace>
    <ned-settings>
      <cisco-ftd xmlns="http://tailf.com/ned/cisco-ftd/meta">
        <connection>
          <ssl>
            <accept-any>true</accept-any>
          </ssl>
        </connection>
        <nedBehavior>
          <autoEndEasySetup>true</autoEndEasySetup>
          <enableAutoDeploy>true</enableAutoDeploy>
        </nedBehavior>
        <log-verbose>true</log-verbose>
      </cisco-ftd>
    </ned-settings>
  </profile>
  </profiles>
  </devices>
</config>
